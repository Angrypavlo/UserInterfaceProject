<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".updatepart.selectedListButtons.EditListActivity">
    <Button
        android:id="@+id/addItems"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Add trips"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </Button>

    <ListView
        android:id="@+id/allListViewUpdates"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="40dp"
        android:dividerHeight="3dp"
        app:layout_constraintBottom_toBottomOf="@+id/addItems"
        app:layout_constraintTop_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.948"
        tools:layout_editor_absoluteX="-16dp"></ListView>
</androidx.constraintlayout.widget.ConstraintLayout>